package module1practice;

public class HashAgileTechQ {
	
	public static void main(String[] args) {
		
		for(int i=1;i<6;i++) {
		int	fvalue=2,lvalue=1;
		int tvalue=fvalue+lvalue;
		for(int j=1;j<=tvalue;j++) {
			System.out.println(tvalue);
			
			
		}
		fvalue=lvalue;
		lvalue=tvalue;
		}
		
	}

}
